pipeline{
 agent any
 tools {
   maven 'Maven 3.3.9'
   jdk 'jdk8'
 }
 stages {
   stage('Checkstyle'){
      steps{
	agent { label "worker-tester" }
	sh 'mvn checkstyle:checkstyle'
      }
   }
   stage('Test'){
      steps{
	agent { label "worker-builder" }
        sh 'mvn test'
      }
   }
   
 }
}
