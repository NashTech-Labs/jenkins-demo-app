pipeline{
 agent all
 tools {
   maven 'Maven 3.3.9'
   jdk 'jdk8'
 }
 stages {
   stage('Checkstyle'){
      agent { label "worker-tester" }
      steps{
	sh 'mvn checkstyle:checkstyle'
      }
   }
   stage('Test'){
      agent { label "worker-builder" }
      steps{
        sh 'mvn test'
      }
   }
   
 }
}
